# 后端 API 地址（请替换为服务器公网 IP + 端口）
API_BASE_URL=http://YOUR_SERVER_IP:3000 # 请替换为服务器公网IP和实际端口（生产环境务必使用公网地址，勿用 localhost）

# 认证密钥（必须与后端 backend/.env 中的 API_KEY 保持一致）
API_KEY=

# 设备 ID 为 pc
DEVICE_ID=pc

# 设备显示名称
DEVICE_NAME=My PC

# 轮询与上报设置（毫秒）
POLL_INTERVAL=2000
UPLOAD_INTERVAL=60000
MIN_USAGE_TIME=5000
HEARTBEAT_INTERVAL=30000
